<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.BakesBay" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">#ff682d</item>
        <item name="colorPrimaryVariant">#ff682d</item>
        <item name="colorOnPrimary">#2021242C</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
    </style>
    <style name="Elcode.Search" parent="@android:style/Widget.EditText">
        <item name="android:textColor">@color/black</item>
        <item name="android:gravity">center</item>
        <item name="android:buttonCornerRadius">10dp</item>
        <item name="android:fontFamily">@font/baloo</item>
    </style>
    <style name="Theme.BakesBay.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="Theme.BakesBay.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.BakesBay.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
</resources>